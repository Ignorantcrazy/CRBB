  <!--Demo 存储过程 查询所有局站信息-->
  <dataCommand name="proc_GetStationId" database="MDATA" commandType="StoredProcedure">
    <commandText>
      <![CDATA[dbo.DAP_GetStationId]]>
    </commandText>
    <parameters>
